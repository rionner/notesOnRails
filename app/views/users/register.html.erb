<%= render partial: 'login_nav' %>

<div>

  <%= form_for @user, class:  'new-user-form' do |f|%>
     <%= f.label :username %>
     <%= f.text_field :username %>
     <%= f.label :password %>
     <%= f.password_field :password %>
     <%= f.label :confirm_password %>
     <%= f.password_field :password_confirmation %>
     <br>
     <%= f.submit_tag "Register" %>
   <% end %>

</div>
